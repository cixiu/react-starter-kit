{
  "name": "web",
  "version": "0.0.0",
  "private": true,
  "engines": {
    "node": ">=6.13.1",
    "npm": ">=3.10.10"
  },
  "browserslist": [
    ">1%",
    "last 4 versions",
    "Firefox ESR",
    "not ie < 9"
  ],
  "scripts": {
    "precommit": "lint-staged",
    "lint-js": "eslint --ignore-path .gitignore --ignore-pattern \"!**/.*\" .",
    "lint-css": "stylelint \"src/**/*.{css,less,styl,scss,sass,sss}\"",
    "lint": "yarn run lint-js && yarn run lint-css",
    "fix-js": "yarn run lint-js --fix",
    "fix-css": "yarn run lint-css --fix",
    "fix": "yarn run fix-js && yarn run fix-css",
    "flow": "flow",
    "flow:check": "flow check",
    "test": "jest",
    "test-watch": "yarn run test --watch --notify",
    "test-cover": "yarn run test --coverage",
    "coverage": "yarn run test-cover && opn coverage/lcov-report/index.html",
    "clean": "babel-node tools/run clean",
    "copy": "babel-node tools/run copy",
    "bundle": "babel-node tools/run bundle",
    "build": "babel-node tools/run build",
    "build-stats": "yarn run build --release --analyse",
    "deploy": "babel-node tools/run deploy",
    "render": "babel-node tools/run render",
    "serve": "babel-node tools/run runServer",
    "start": "babel-node tools/run start"
  },
  "dependencies": {
    "@babel/polyfill": "^7.0.0",
    "antd": "^3.11.0",
    "axios": "^0.18.0",
    "body-parser": "^1.18.3",
    "classnames": "^2.2.6",
    "cookie-parser": "^1.4.3",
    "dotenv": "^6.2.0",
    "express": "^4.16.3",
    "history": "^4.7.2",
    "isomorphic-style-loader": "^4.0.0",
    "pretty-error": "^2.1.1",
    "prop-types": "^15.6.2",
    "query-string": "5",
    "react": "^16.6.3",
    "react-dom": "^16.6.3",
    "react-redux": "^6.0.0",
    "redux": "^4.0.1",
    "redux-devtools-extension": "^2.13.7",
    "redux-thunk": "^2.3.0",
    "serialize-javascript": "^1.5.0",
    "universal-router": "^7.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.2.0",
    "@babel/node": "^7.2.0",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/preset-env": "^7.2.0",
    "@babel/preset-react": "^7.0.0",
    "@types/body-parser": "^1.17.0",
    "@types/cookie-parser": "^1.4.1",
    "@types/dotenv": "^6.1.0",
    "@types/express": "^4.16.0",
    "@types/history": "^4.7.2",
    "@types/query-string": "^6.1.1",
    "@types/react": "^16.7.13",
    "@types/react-dom": "^16.0.11",
    "@types/react-redux": "^6.0.10",
    "@types/serialize-javascript": "^1.5.0",
    "@types/universal-router": "^6.0.1",
    "autoprefixer": "^9.4.2",
    "babel-jest": "^23.6.0",
    "chalk": "^2.4.1",
    "chokidar": "^2.0.4",
    "css-loader": "^1.0.1",
    "enzyme": "^3.7.0",
    "extract-css-chunks-webpack-plugin": "^3.2.1",
    "file-loader": "^2.0.0",
    "fork-ts-checker-webpack-plugin": "^0.5.0",
    "front-matter": "^3.0.1",
    "glob": "^7.1.3",
    "husky": "^1.2.0",
    "jest": "^23.6.0",
    "less": "3.9.0",
    "less-loader": "4.1.0",
    "lint-staged": "^8.1.0",
    "markdown-it": "^8.4.2",
    "mkdirp": "^0.5.1",
    "null-loader": "^0.1.1",
    "opn-cli": "^4.0.0",
    "optimize-css-assets-webpack-plugin": "^5.0.1",
    "ora": "^3.0.0",
    "pleeease-filters": "^4.0.0",
    "postcss-loader": "^3.0.0",
    "prettier": "^1.15.3",
    "raw-loader": "^0.5.1",
    "react-deep-force-update": "^2.1.3",
    "react-dev-utils": "^6.1.1",
    "react-error-overlay": "^5.1.0",
    "react-test-renderer": "^16.6.3",
    "rimraf": "^2.6.2",
    "source-map-support": "^0.5.9",
    "stylelint": "^9.9.0",
    "stylelint-config-standard": "^18.2.0",
    "stylelint-order": "^2.0.0",
    "svg-url-loader": "^2.3.2",
    "terser-webpack-plugin": "^1.1.0",
    "thread-loader": "^1.2.0",
    "ts-import-plugin": "1.5.5",
    "ts-loader": "^5.3.1",
    "tslint": "^5.11.0",
    "tslint-loader": "^3.5.4",
    "tslint-react": "^3.6.0",
    "typescript": "^3.2.1",
    "typings-for-css-modules-loader": "^1.7.0",
    "url-loader": "^1.1.2",
    "webpack": "^4.27.1",
    "webpack-assets-manifest": "^3.1.1",
    "webpack-bundle-analyzer": "^3.0.3",
    "webpack-dev-middleware": "^3.4.0",
    "webpack-hot-middleware": "^2.24.3",
    "webpack-node-externals": "^1.7.2",
    "write-file-webpack-plugin": "^4.5.0"
  },
  "lint-staged": {
    "ignore": [
      "package.json"
    ],
    "linters": {
      "*.{js,jsx}": [
        "eslint --no-ignore --fix",
        "git add --force"
      ],
      "*.{json,md,graphql}": [
        "prettier --write",
        "git add --force"
      ],
      "*.{css,less,styl,scss,sass,sss}": [
        "stylelint --fix",
        "git add --force"
      ]
    }
  }
}
